import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		
		int len = sc.nextInt();
		String skip = sc.nextLine();
		
		for(int i = 1; i <= len; i++) {
			String first = sc.nextLine();
			
			int first1 = first.length();
			
			char num1_2 = first.charAt(first1 - 1);
			char num1_1 = first.charAt(0);
			System.out.print(num1_1); System.out.println(num1_2);
		}
	}

}
