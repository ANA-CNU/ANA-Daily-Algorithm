#include <stdio.h>

int main() {
	int p_23 = 0;
	int p_2323 = 0;
	int p_232323 = 0;
	int p_23232323 = 0;
	int P,T,result;

	scanf("%d", &T);

	for (; T > 0; T--) {
		scanf("%d", &P);
		p_23 = P;
		if (p_23 > 101) {
			p_23 -= 101;
			p_2323++;
			if (p_2323 > 100) {
				p_2323 -= 100;
				p_232323++;
			}
			if (p_232323 > 100) {
			p_232323 -= 100;
			p_23232323++;
			}
		}
		result = p_23 * 23 + p_2323 * 2323 + p_232323 * 232323 + p_23232323 * 23232323;
		printf("%d\n",result);
		p_23 = p_2323 =p_232323 =p_23232323 = 0;
	}


}
